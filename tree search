import java.util.ArrayList;
import java.util.List;

public class Node {
    public int value;
    public List<Node> children;

    public Node(int value) {
        this.value = value;
        this.children = new ArrayList<>();
    }
}

public class TreeSearch {

    public static List<Node> getParents(Node rootNode, int valueToSearch) {
        List<Node> path = new ArrayList<>();
        dfs(rootNode, valueToSearch, path);
        return path;
    }

    private static boolean dfs(Node node, int valueToSearch, List<Node> path) {
        if (node.value == valueToSearch) {
            path.add(node);
            return true;
        }
        path.add(node);
        for (Node child : node.children) {
            if (dfs(child, valueToSearch, path)) {
